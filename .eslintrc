{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "consistent-return": "error",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn"
  }
}
